pipeline{
    agent any
    stages{
        stage('one'){
            steps{
                echo 'post actions'
            }
        }
    }
    post{
        always{
            echo 'always'
        }
        sucess{
            echo 'sucess'
        
        }
        failure{
            echo 'failure'
        }
        unstable{
            echo 'unstable'
        }
        changed{
            echo 'changed'
        }
        aborted{
            echo 'aborted'
        }
    }
}
